   "homepage": "https://github.com/marcovo/phpbb_mathjax",
   "version": "1.0.0",
   "license": "GPL-2.0",
   "authors": [
      {
         "name": "Marco van Oort",
         "email": "marcovoort@gmail.com",
         "homepage": "https://github.com/marcovo",
         "role": "Developer"
      }
   ],
   "require": {
      "php": ">=5.3.3"
   },
   "extra": {
      "display-name": "MathJax phpBB Integration",
      "soft-require": {
         "phpbb/phpbb": ">=3.1.3,<3.2.*@dev"
      }
   }
}