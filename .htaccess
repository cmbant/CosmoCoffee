# Security: Block access to sensitive files and directories
# Modern Apache 2.4+ syntax (most servers support this now)

# Block access to sensitive PHP files
<Files ~ "^(config|common)\.php$">
	Require all denied
</Files>

# Block access to sensitive file extensions
<FilesMatch "\.(md|ini|git.*|sql|gz|lock|json|yml|yaml)$">
	Require all denied
</FilesMatch>

# Block access to sensitive directories
<DirectoryMatch "^.*(scripts|vendor|phpbb|config|includes|\.git|bin|docs).*$">
	Require all denied
</DirectoryMatch>

# Block access to hidden files (starting with .)
<FilesMatch "^\.">
	Require all denied
</FilesMatch>

# Block access to backup and temporary files
<FilesMatch "\.(bak|backup|tmp|temp|log|old|orig|save|swp|swo)$">
	Require all denied
</FilesMatch>
